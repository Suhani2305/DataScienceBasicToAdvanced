for letter in "Python":
    if letter == "h":
        # continue  # Skip the rest of the loop for this iteration
        # pass # Using pass instead of continue to do nothing for this iteration
        break # Exit the loop entirely if 'h' is encountered
    print(letter)  # This will print all letters except 'h'